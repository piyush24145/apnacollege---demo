// Online C compiler to run C program online
#include <stdio.h>

int main()
{ float a,b,c,avg=0;
printf("enter the value a:");
scanf("%f",&a);
printf("enter the value b:");
scanf("%f",&b);
printf("enter the value c:");
scanf("%f",&c);
avg=(a+b+c)/3;
printf("average of three number is %f",avg);

 


    return 0;
    
}
